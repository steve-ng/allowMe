{
  "name": "allow-me",
  "version": "0.1.1",
  "description": "Visual aid for your users in giving you browser's permission such as cam or mic",
  "main": "allowMe.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/steve-ng/allowMe.git"
  },
  "author": "Steve Ng",
  "license": "MIT",
  "homepage": "https://github.com/steve-ng/allowMe",
  "devDependencies": {
    "grunt-contrib-uglify": "~0.6.0",
    "grunt-contrib-cssmin": "~0.10.0",
    "load-grunt-tasks": "~1.0.0"
  },
  
  "npmName": "allow-me",
  "npmFileMap": [{
    "basePath": "/dist/",
    "files": [
      "*.js",
      "*.css"
    ]
  }],

  "autoupdate": {
    "source": "git",
    "target": "git://github.com/steve-ng/allowMe.git",
    "basePath": "/dist/",
